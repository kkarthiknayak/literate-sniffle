pipeline{
    agent any

    stages{
        stage('checkout'){
            steps {
                echo 'checkout'
            }
        }
        stage('build'){
            steps {
                echo 'building'
            }
        }
        stage('test'){
            steps {
                echo 'testing'
            }
        }
        stage('deploy'){
            steps {
                echo 'deploying'
            }
        }
    }
}
